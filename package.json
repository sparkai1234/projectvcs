{
  "name": "supabase-vc-fund-scrapers",
  "version": "2.0.0",
  "description": "Protected Supabase VC and Fund scrapers with data protection logic",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "start:vc": "node supabase_vc_scraper_protected.js",
    "start:fund": "node supabase_fund_scraper_protected.js",
    "start:fund-updated": "node main.js",
    "supabase:vc": "node run_supabase_vc_scraper.js",
    "supabase:fund": "node run_supabase_fund_scraper.js",
    "supabase:vc-working": "node supabase_vc_scraper_working.js",
    "supabase:fund-working": "node supabase_fund_scraper_working.js",
    "supabase:test": "node test_supabase_connection.js",
    "diva:scrape": "python diva_html_scraper.py",
    "diva:supabase": "python diva_to_supabase.py",
    "diva:test": "python test_diva_supabase.py",
    "diva:all": "npm run diva:scrape && npm run diva:supabase",
    "diva:setup-raw": "python populate_raw_tables.py",
    "diva:migrate-raw": "python migrate_to_raw_tables.py",
    "diva:verify": "python verify_supabase_data.py",
    "diva:check-structure": "python check_raw_data_structure.py",
    "diva:verify-complete": "python verify_raw_tables_complete.py",
    "diva:vcmap": "node scrapers/diva_vcmap_scraper.js",
    "diva:violations": "node scrapers/diva_violations_scraper.js",
    "diva:reports": "node scrapers/diva_market_reports_downloader.js",
    "diva:comprehensive": "npm run diva:vcmap && npm run diva:violations && npm run diva:reports",
    "kdb:fof": "node scrapers/kdb_fund_of_funds_scraper.js",
    "kgrowth:fof": "node scrapers/kgrowth_fund_of_funds_scraper.js",
    "kvic:api": "node scrapers/kvic_api_scraper.js",
    "fof:all": "npm run kdb:fof && npm run kgrowth:fof && npm run kvic:api",
    "pdf:analyze": "node scrapers/pdf_ai_analyzer.js",
    "pdf:create-dir": "if not exist pdf_reports mkdir pdf_reports && echo PDF reports directory created. Add KDB and K-Growth annual reports here.",
    "ai:all": "npm run pdf:analyze",
    "trends:generate": "node analysis/comprehensive_trend_reporter.js",
    "trends:create-dir": "mkdir -p trend_reports && echo 'Trend reports directory created.'",
    "analysis:all": "npm run trends:generate",
    "db:create-fof-tables": "psql $DATABASE_URL -f sql/create_fund_of_funds_tables.sql",
    "db:create-kvic-tables": "psql $DATABASE_URL -f sql/create_kvic_tables.sql",
    "db:create-pdf-tables": "psql $DATABASE_URL -f sql/create_pdf_analysis_tables.sql",
    "db:setup-fof": "npm run db:create-fof-tables && npm run db:create-kvic-tables",
    "db:setup-ai": "npm run db:create-pdf-tables",
    "db:setup-all": "npm run db:setup-fof && npm run db:setup-ai",
    "db:verify": "node verify_tables.js",
    "db:setup": "node setup_database.js",
    "test": "echo \"No tests available\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "apify": "^3.1.0",
    "apify-client": "^2.8.0",
    "crawlee": "^3.13.0",
    "dotenv": "^16.3.0",
    "openai": "^4.104.0",
    "pdf-parse": "^1.1.1",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "DIVA Intelligence System",
  "license": "MIT",
  "keywords": [
    "apify",
    "supabase",
    "vc-scraper",
    "fund-scraper",
    "diva-scraper",
    "data-protection",
    "korean-vc-database",
    "personnel-intelligence",
    "competitive-intelligence",
    "risk-assessment",
    "pdf-analysis",
    "openai-integration",
    "trend-analysis",
    "fund-of-funds"
  ]
}
